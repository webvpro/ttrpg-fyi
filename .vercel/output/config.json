{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/_image/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/about/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/account/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/login/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/oauth/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/signup/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/$",
			"dest": "_middleware"
		}
	]
}